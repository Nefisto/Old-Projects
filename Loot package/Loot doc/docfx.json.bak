{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Loot_Scripts.csproj"
          ],
		  "src": "../"
        }
      ],
      "dest": "api",
	  "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
		  "manual/**.md",
		  "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "manual/images/**"
        ]
      }
    ],
    "dest": "docs",
    "template": [
      "default",
	  "templates/unity"
    ]
  }
}